.swiper {
  border-bottom: 1px solid #ccc;
  width: 100%;
  height: 400rpx;
}

swiper image {
  height: 100%;
  width: 200px;
}

.imageBanner {
  margin-top: 10px;
  width: 100%;
  height: 100%;
}

.imageBanner_item {
  width: 100%;
  height: 200rpx;
}

.swiper-item {
  display: block;
  width: 100%;
  height: 200px;
}

.car_wrapper{
  width: 100%;
  margin-top: 10px;
  background-color: #fff;
  box-shadow: 0 1px 2px rgba(0, 0, 0, 0.1);
}

.car_cell{
  width: 50%;
  padding: 20rpx;
  display: inline-block;
  vertical-align: middle;
  box-sizing: border-box;
  overflow: hidden;
}

.web_title{
  font-size: 20px;
  white-space: nowrap;
}

.web_sub{
  font-size: 13px;
  color: #ccc;
  white-space: nowrap;
}

.left_card{
  width: 65%;
  text-align: left;
  display: inline-block;
  vertical-align: middle;
  box-sizing: border-box;
  padding: 0 10px;
  overflow: hidden;
}

.right_card{
  width: auto;
  display: inline-block;
  vertical-align: middle;
  /*margin-right: 10px;*/
  overflow: hidden;
}

.right_card image{
  width: 50px;
  height: 50px;
}